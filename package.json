{
  "name": "storefront-api",
  "version": "1.0.0",
  "description": "StoreFront API",
  "main": "index.js",
  "scripts": {
    "test": "export ENV=test && db-migrate down && db-migrate up --env test && npx tsc && jasmine &&db-migrate reset",
    "dev": "db-migrate down && db-migrate up && nodemon ./src/index.ts",
    "build": "npx tsc",
    "start": "db-migrate down && db-migrate up && npm run build && node build/index.js",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "lint": "eslint . --ext .ts",
    "git":"git status && git add . && git commit -m 'update' && git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abuiliazeed/StorefrontAPI.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abuiliazeed/StorefrontAPI/issues"
  },
  "homepage": "https://github.com/abuiliazeed/StorefrontAPI#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.26",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.99.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
